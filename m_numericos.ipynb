        "import math\n",
        "\n",
        "def myexp(x):\n",
        "  e=0\n",
        "  for i in range(0,15):\n",
        "    e=e+(x**i)/math.factorial(i)\n",
        "  return e\n",
        "\n",
        "print(f\"Aproximación de raíz de e con Maclaurin: {myexp(0.5)}\")\n",
        "print(\"Fueron usados 15 \")\n"
      ],
